{
  "name": "astal-config",
  "version": "0.0.0",
  "description": "Astal Config",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "effect": "$effect",
      "typescript": "$typescript"
    }
  },
  "scripts": {},
  "dependencies": {
    "effect": "3.11.1"
  },
  "devDependencies": {
    "prettier": "3.4.2",
    "typescript": "5.7.2"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  },
  "madge": {
    "tsConfig": {
      "compilerOptions": {
        "baseUrl": "."
      }
    },
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  }
}
