{
  "name": "astal-config",
  "version": "0.0.0",
  "description": "Astal Config",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "pnpm": "9.15.0"
  },
  "pnpm": {
    "overrides": {
      "effect": "$effect",
      "typescript": "$typescript"
    }
  },
  "scripts": {
    "clean": "rimraf -g packages/*/dist packages/core/*/dist .eslintcache **/.tsbuildinfo **/.rollup.cache"
  },
  "dependencies": {
    "astal": "github:Aylur/astal#path:/lang/gjs",
    "effect": "3.11.1"
  },
  "devDependencies": {
    "eslint": "^9.16.0",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "5.7.2"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  },
  "madge": {
    "tsConfig": {
      "compilerOptions": {
        "baseUrl": "."
      }
    },
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  }
}
